public class Tester {

    public boolean createShip() {

        try {
            Ship ship = new Ship("shipTest");
            ship.description = "A test ship.";
            ship.hold = new Commodity[5];
            ship.crew = 5;
            return true;
        } catch (Exception e) {
            return false;
        }
    }
}
